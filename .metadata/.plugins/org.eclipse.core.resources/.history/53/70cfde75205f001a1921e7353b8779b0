package initialMaker;
import java.util.Scanner;
public class initialMaker 
{
	public static void main(String args[])
	{
		initialMaker();
	}
	public static String initialMaker()
	{
		String init = null;
		Scanner scan = new Scanner(System.in);
		String name1 = scan.next();
		init += name1.charAt(0);
		return init;
	}
	
	
}
